{
  "expo": {
    "name": "desapeguitos",
    "slug": "desapeguitos",
    "owner": "tgtech",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "desapeguitos",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "10",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Este aplicativo usa sua localização para mostrar desapegos relevantes com base em sua localização.",
        "NSLocationAlwaysUsageDescription": "Este aplicativo precisa acessar sua localização em segundo plano para fornecer uma experiência contínua.",
        "NSCameraUsageDescription": "Este aplicativo precisa acessar a câmera para permitir que você tire fotos."
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.tamaramontijo.desapeguitos"
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tamaramontijo.desapeguitos"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-apple-authentication"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "62be2334-7dcc-44e8-a0a5-d25070f5dd70"
      }
    }
  }
}
